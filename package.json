{
  "name": "react-d3-forces",
  "version": "0.0.1",
  "description": "A wrapper around d3.force to use in in React Apps.",
  "author": "Will Buchanan <willwritescode@gmail.com",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "keywords": [
    "react",
    "utility"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "build": "tsc",
    "lint": "eslint --fix src test",
    "validate": "npm run lint && npm run coverage && npm run build && npm run size",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/index.js"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "lodash": "^4.17.21",
    "selective-context": "2.0.1"

  },
  "peerDependencies": {
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@babel/cli": "^7.24.1",
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@eslint/js": "^9.2.0",
    "@size-limit/preset-small-lib": "^11.1.3",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-istanbul": "^1.6.0",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^9.2.0",
    "eslint-plugin-n": "^17.7.0",
    "eslint-plugin-vitest": "^0.5.4",
    "jsdom": "^24.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-json-tree": "^0.19.0",
    "size-limit": "^11.1.3",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.8.0",
    "vitest": "^1.6.0"
  }
}
